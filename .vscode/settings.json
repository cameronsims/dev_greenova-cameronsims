{
    "[css]": {
        "editor.defaultFormatter": "vscode.css-language-features",
        "editor.formatOnSave": false,
        "editor.suggest.insertMode": "replace",
        "editor.tabSize": 2,
        "editor.insertSpaces": true
    },
    "[git-commit]": {
        "editor.rulers": [
            72,
            50
        ],
        "editor.wordWrap": "on",
        "editor.tabSize": 2,
        "editor.insertSpaces": true,
        "editor.fontFamily": "monospace"
    },
    "[html]": {
        "editor.formatOnSave": true,
        "editor.defaultFormatter": "vscode.html-language-features",
        "editor.tabSize": 2,
        "editor.insertSpaces": true,
        "editor.suggest.insertMode": "replace"
    },
    "[javascript]": {
        "editor.defaultFormatter": "dbaeumer.vscode-eslint",
        "editor.formatOnSave": true,
        "editor.tabSize": 2,
        "editor.insertSpaces": true
    },
    "[json]": {
        "editor.formatOnSave": true,
        "editor.defaultFormatter": "vscode.json-language-features",
        "editor.tabSize": 2,
        "editor.insertSpaces": true
    },
    "[markdown]": {
        "editor.formatOnSave": true,
        "editor.defaultFormatter": "vscode.markdown-language-features",
        "editor.wordWrap": "on",
        "editor.quickSuggestions": {
            "comments": "off",
            "strings": "off",
            "other": "off"
        }
    },
    "[pip-requirements]": {
        "editor.defaultFormatter": "ms-python.python",
        "editor.insertFinalNewline": true,
        "editor.tabSize": 4,
        "editor.insertSpaces": true,
        "files.trimTrailingWhitespace": true
    },
    "[plaintext]": {
        "editor.wordWrap": "on",
        "editor.quickSuggestions": {
            "comments": false,
            "strings": false,
            "other": false
        },
        "editor.tabSize": 4,
        "files.insertFinalNewline": true
    },
    "[python]": {
        "editor.formatOnSave": true,
        "editor.defaultFormatter": "ms-python.python",
        "editor.codeActionsOnSave": {
            "source.organizeImports": "explicit",
            "source.fixAll": "explicit"
        },
        "editor.rulers": [
            88,
            100
        ],
        "editor.wordBasedSuggestions": "off"
    },
    "[shellscript]": {
        "editor.defaultFormatter": "foxundermoon.shell-format",
        "editor.formatOnSave": true,
        "files.eol": "\n"
    },
    "editor.bracketPairColorization.enabled": true,
    "editor.guides.bracketPairs": true,
    "editor.formatOnSave": true,
    "editor.formatOnPaste": true,
    "editor.linkedEditing": true,
    "editor.renderWhitespace": "boundary",
    "editor.rulers": [
        79,
        88,
        100
    ],
    "files.autoSave": "afterDelay",
    "files.autoSaveDelay": 500,
    "files.watcherExclude": {
        "**/.git/objects/**": true,
        "**/.git/subtree-cache/**": true,
        "**/node_modules/**": true,
        "**/.venv/**": true,
        "**/__pycache__/**": true
    },
    "files.watcherInclude": [
        "**/*.py",
        "**/*.html",
        "**/*.css",
        "**/*.js"
    ],
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/*.pyc": true,
        "**/__pycache__": true,
        "**/.pytest_cache": true,
        "**/node_modules": true,
        "**/.venv": true
    },
    "extensions.ignoreRecommendations": true,
    "python.analysis.diagnosticMode": "workspace",
    "python.analysis.indexing": false,
    "javascript.suggest.enabled": false,
    "typescript.suggest.enabled": false,
    "git.autofetch": true,
    "git.confirmSync": false,
    "git.enableSmartCommit": true,
    "github.copilot.advanced": {
        "listCount": 10,
        "inlineSuggestCount": 3,
        "debug.enable": false
    },
    "github.copilot.chat.codeGeneration.useInstructionFiles": true,
    "github.copilot.editor.enableAutoCompletions": true,
    "github.copilot.enable": {
        "*": true,
        "plaintext": true,
        "markdown": true,
        "scminput": false
    },
    "github.copilot.nextEditSuggestions.enabled": true,
    "html.format.templating": true,
    "task.reconnection": true,
    "workbench.colorTheme": "Default Dark Modern",
    "workbench.editor.enablePreview": false,
    "debug.inlineValues": "auto",
    "debug.console.fontSize": 14,
    "debug.toolBarLocation": "docked",
    "debugpy.debugJustMyCode": false,
    "editor.stickyScroll.enabled": true,
    "eslint.validate": [
        "javascript"
    ],
    "eslint.format.enable": true,
    "files.associations": {
        "*.html": "html",
        "*.js": "javascript",
        "*.py": "python",
        "requirements*.txt": "pip-requirements"
    },
    "html.format.wrapLineLength": 88,
    "html.format.wrapAttributes": "preserve-aligned",
    "javascript.format.semicolons": "insert",
    "javascript.preferences.quoteStyle": "single",
    "javascript.updateImportsOnFileMove.enabled": "always",
    "python.analysis.autoImportCompletions": false,
    "python.analysis.extraPaths": [
        "${workspaceFolder}/.venv/lib/python3.13/site-packages"
    ],
    "python.analysis.typeCheckingMode": "basic",
    "python.terminal.activateEnvInCurrentTerminal": true,
    "python.terminal.shellIntegration.enabled": true,
    "python.terminal.activateEnvironment": true,
    "python.analysis.inlayHints.functionReturnTypes": false,
    "python.analysis.inlayHints.variableTypes": false,
    "python.testing.unittestEnabled": true,
    "python.testing.unittestArgs": [
        "-v",
        "-s",
        "./greenova",
        "-p",
        "*test*.py"
    ],
    "python.envFile": "${workspaceFolder}/.env",
    "eslint.codeActionsOnSave.rules": [
        "!prettier/*",
        "*"
    ],
    "python.testing.pytestEnabled": false,
    "remoteHub.gitProtocol": "ssh",
    "github.gitProtocol": "ssh",
    "python.analysis.autoFormatStrings": true,
    "python.analysis.completeFunctionParens": true,
    "python.analysis.enablePytestSupport": false,
    "python.analysis.languageServerMode": "light",
    "python.analysis.logLevel": "Error",
    "terminal.integrated.cwd": "${workspaceFolder}",
    "terminal.integrated.defaultProfile.linux": "bash",
    "terminal.integrated.shellIntegration.enabled": true,
    "terminal.integrated.env.linux": {
        "PYTHONPATH": "${workspaceFolder}/.venv/lib/python3.13/site-packages",
        "PATH": "${workspaceFolder}/.venv/bin:${env:PATH}"
    }
}